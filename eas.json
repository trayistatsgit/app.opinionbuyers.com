{
  "cli": {
    "version": ">= 12.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
        "API_URL": "https://dev.api.example.com"
      }
      },

      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "API_URL": "https://api.example.com"
        }
      },
      "ios": {
        "simulator": false
      }

    }
  },
  "submit": {
    "production": {}
  }
}
